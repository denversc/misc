[user]
  name = Denver Coneybeare
[color]
  ui = auto
[core]
  eol = nl
  editor = vim
[alias]
  su = status -uall
  co = checkout
  dt = difftool
  dtc = difftool --cached
  dc = diff --cached
  st = status
  lf = diff-tree --no-commit-id --name-only -r
[push]
  default = upstream
[pull]
  ff = only
